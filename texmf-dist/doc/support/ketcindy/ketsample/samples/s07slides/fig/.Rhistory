Makevaltable<- function(MS){  Eps=10^(-6)  Nargs=length(MS)  Fstr=Op(1,MS)  Tmp=Op(2,MS)  Tmp1=strsplit(Tmp,'=',fixed=TRUE)  Xname=Tmp1[1]  Xrange=eval(parse(text=Tmp1[2]))  Tmp=Op(3,MS)  Tmp1=strsplit(Tmp,'=',fixed=TRUE)  Yname=Tmp1[1]  Yrange=evstr(parse(text=Tmp[2]))  Tmp=gsub(Xname,"x",Fstr)  Fstr=gsub(Yname,"y",Tmp)  Tmp=grep(Xname,Fstr)  if(Tmp==0){    Fstr=paste(Fstr,'+0*x',sep="")  }  Mdv=50  Ndv=50  if(Nargs>=4){    Tmp=Op(4,MS)    if(nrow(Tmp)>1){      Mdv=Tmp[1,1]      Ndv=Tmp[1,2]    }else{      Mdv=Tmp      if(Nargs==4){        Ndv=Mdv      }else{        Tmp1=Op(5,MS)        if((is.numeric(Tmp1))&&(length(Tmp1)==1)){          Ndv=Tmp1        }else{          Ndv=Mdv        }      }    }  }  X1=Xrange[1]; X2=Xrange[2]  Y1=Yrange[1]; Y2=Yrange[2]  Dx=(X2-X1)/Mdv  Dy=(Y2-Y1)/Ndv  Xval=c()  Tmp=X1  while(Tmp<X2+Eps){    Xval=c(Xval,Tmp)    Tmp=Tmp+Dx  }  Yval=c()  Tmp=Y1  while(Tmp<Y2+Eps){    Yval=c(Yval,Tmp)    Tmp=Tmp+Dy  }  I=1  for(y in Yv
al){    J=1    for(x in Xval){      Zval[I,J]=eval(parse(text=Fstr))      J=J+1    }    I=I+1  }  return(list(Zval,Xval,Yval))}
Implicitplot<- function(...){  varargin=list(...)  Eps=10^(-4)  Nargs=length(varargin)  Fstr=varargin[[1]]  if(!is.numeric(Fst)){    MS=Fstr    Tmp=Makevaltable(MS)    Zval=Tmp[[1]]; Xval=Tmp[[2]]; Yval=Tmp[[3]]  }else if(is.character(Fstr)){    MS=list()    for(I in 1:Nargs){      MS=c(MS,varargin[i])    }    Tmp=Makevaltable(MS)    Zval=Tmp[[1]]; Xval=Tmp[[2]]; Yval=Tmp[[3]]  }else{    Zval=varargin[[1]]    Xval=varargin[[2]]    Yval=varargin[[3]]  }  Out=c()  for(J in Looprange(1,length(Yval)-1)){    for(I in Looprange(1,length(Xval)-1)){      a1=Xval[I]; b1=Yval[J]; c1=Zval[J,I]      a2=Xval[I+1]; b2=Yval[J]; c2=Zval[J,I+1]      a3=Xval[I+1]; b3=Yval[J+1]; c3=Zval[J+1,I+1]      a4=Xval(I);b4=Yval(J+1);c4=Zval(J+1,I);      PL=as.matrix(c(a1,b1,a2,b2,a3,b3,a4,b4,a1,b1),ncol=2,byrow=FALSE)      VL=c(c1,c2,c3,c4,c1)      QL=c()      for(K in 1:4){        if(abs(VL[K])<=Eps){          QL=appendrow(QL,PL[K,])        }else if(VL[K]>Eps){          if(VL[K+1]<-Eps){
Tmp=1/(VL[K]-VL[K+1])*(-VL[K+1]*PL[K,]+VL[K]*PL[K+1,])            QL=appendrow(QL,Tmp)          }        }else{          if(VL[K+1]>Eps){            Tmp=1/(-VL[K]+VL[K+1])*(VL[K+1]*PL[K,]-VL[K]*PL[K+1,])            QL=appendrow(QL,Tmp)          }        }      }      if(nrow(QL)==2){        Out=append(Out,c(Inf,Inf),QL[1,],QL[2,])      }    }  }  Out=Out[2:nrow(Out),]  if(nrow(Out)>0){    Out=Connectseg(Out)  }else{    Out=c()  }  retuirn(Out)}
